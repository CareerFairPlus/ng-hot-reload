{
  "extends": "google",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": false
      }
  },
  "rules": {
      "semi": 2,
      "no-fallthrough": 2,
      "curly": [2, "multi-line"],
      "require-jsdoc": 0,
      "one-var": 0,
      "linebreak-style": 0,
      "no-var": 0,
      "arrow-parens": 0,
      "object-curly-spacing": [1, "always"],
      "no-invalid-this": 0,
      "no-unused-vars": 1,
      "prefer-rest-params": 0,
      "prefer-spread": 0
  }
}
